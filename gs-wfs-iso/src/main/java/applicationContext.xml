	<!-- default feature type encoders -->
	<bean id="gml2FeatureTypeEncoderISO" 
		class="org.geoserver.wfs.xml.v1_1_0.XmlSchemaEncoder_ISO$V10">
		<constructor-arg ref="geoServer"/>
	</bean>
	<bean id="gml3FeatureTypeEncoderISO" 
		class="org.geoserver.wfs.xml.v1_1_0.XmlSchemaEncoder_ISO$V11">
		<constructor-arg ref="geoServer"/>
	</bean>
	<bean id="gml32FeatureTypeEncoderISO" 
        class="org.geoserver.wfs.xml.v1_1_0.XmlSchemaEncoder_ISO$V20">
        <constructor-arg ref="geoServer"/>
    </bean>
    
	<!-- default get feature producers -->
	<bean id="gml2OutputFormatISO" 
		class="org.geoserver.wfs.xml.GML2OutputFormat_ISO" scope="prototype">
		<constructor-arg ref="geoServer"/>
	</bean>
	
</beans>
